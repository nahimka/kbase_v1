<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<h1>Your Pragmatic Catalog</h1>

<% @posts.each do |post| %>
  <div class="entry">
    
    <h3><%= post.title %></h3>
    <%= sanitize(post.body) %>
    <div class="price_line">
      <span class="price"><%= post.version %></span>
    </div>
  </div>
<% end %>


